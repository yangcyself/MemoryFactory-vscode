{
	"name": "memory-factory",
	"displayName": "Memory Factory",
	"description": "Helper for periodically reviewing",
	"version": "0.0.1",
	"publisher": "yangcyself",
	"enableProposedApi": true,
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:MF-all-documents"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "memory-factory-explorer",
					"title": "Memory Factory",
					"icon": "media/ji.svg"
				}
			]
		},
		"views": {
			"memory-factory-explorer": [
				{
					"id": "MF-all-documents",
					"name": "All documents"
				}
			]
		},
		"viewsWelcome": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.45.0",
		"typescript": "^3.9.4",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0"
	},
	"dependencies": {
		"jsonc-parser": "^0.4.2",
		"ftp": "^0.3.10",
		"mongoose": "^5.9",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2"
	}
}
